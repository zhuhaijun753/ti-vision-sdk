    /*-----------------------------------------------------------------------------------*/
    /* program data_reg_phy_wr_dqs_slave_ratio  Write DQS  */
    /*-----------------------------------------------------------------------------------*/

    //- data0 cs0 cntl_0
    WR_MEM_32(DDR0_DATA0_CS0_CTRL0, RD_MEM_32(DDR0_DATA0_CS0_CTRL0) & 0xFFF003FF );
    WR_MEM_32(DDR0_DATA0_CS0_CTRL0, RD_MEM_32(DDR0_DATA0_CS0_CTRL0) | (PHY_WR_DQS << 10) );
