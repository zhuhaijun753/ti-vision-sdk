    /*-----------------------------------------------------------------------------------*/

    //- data0 cs0 cntl_0
    WR_MEM_32(DDR0_DATA0_CS0_CTRL0, RD_MEM_32(DDR0_DATA0_CS0_CTRL0) & 0xFFFFFC00 );
