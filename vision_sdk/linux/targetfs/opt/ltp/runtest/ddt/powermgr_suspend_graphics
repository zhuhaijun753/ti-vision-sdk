# @name 3D Graphics plus suspend/resume
# @desc Display 3D Graphics while doing suspend/resume operations
# @requires graphics && power_mgr
# @setup_requires 

POWERMGR_S_FUNC_GRAPHICS_SUSPEND source "common.sh"; run_processes.sh -c " (which runOGLES2ChameleonMan.sh && runOGLES2ChameleonMan.sh -qaf=3000) || die 'runOGLES2ChameleonMan is not available' # /opt/ltp/runltp -P $PLATFORM -f ddt/powermgr_suspend -s 'POWERMGR_S_FUNC_SIMPLE_SUSPEND ' "

POWERMGR_M_FUNC_GRAPHICS_SUSPEND source "common.sh"; run_processes.sh -c " (which runOGLES2ChameleonMan.sh && runOGLES2ChameleonMan.sh -qaf=30000) || die 'runOGLES2ChameleonMan is not available' # /opt/ltp/runltp -P $PLATFORM -f ddt/powermgr_suspend -s 'POWERMGR_L_STRESS_SIMPLE_SUSPEND ' "