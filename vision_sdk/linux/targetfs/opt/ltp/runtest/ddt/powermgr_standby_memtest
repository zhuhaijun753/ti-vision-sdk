# @name Memtester plus standby/resume
# @desc Test memory using memtester application while doing power standby/resume operations
# @requires power_mgr 
# @setup_requires 

POWERMGR_S_FUNC_MEMTEST10p_STANDBY source "common.sh"; run_processes.sh -c " /opt/ltp/runltp -f ddt/memtest -P $PLATFORM -s 'MEMORY_M_FUNC_MEMTESTER_10pct_1time '  # /opt/ltp/runltp -P $PLATFORM -f ddt/powermgr_suspend -s 'POWERMGR_S_FUNC_SIMPLE_STANDBY ' "

POWERMGR_M_FUNC_MEMTEST50p_STANDBY source "common.sh"; run_processes.sh -c " /opt/ltp/runltp -f ddt/memtest -P $PLATFORM -s 'MEMORY_M_FUNC_MEMTESTER_50pct_1time '  # /opt/ltp/runltp -P $PLATFORM -f ddt/powermgr_suspend -s 'POWERMGR_L_STRESS_SIMPLE_STANDBY ' "